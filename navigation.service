[Unit]
Description=Navigation Data Server
Requires=network.target

[Service]
Type=simple
User=solidsense
ExecStart=/usr/bin/python3  /data/solidsense/navigation/navigation_server.py --settings /data/solidsense/navaigation/settings.yml
Restart=always
RestartSec=6

[Install]
WantedBy=multi-user.target