[Unit]
Description=Navigation Data Server
Requires=network.target

[Service]
Type=simple
User=solidsense
ExecStart=/usr/bin/python3  /data/solidsense/navigation/src/navigation_server.py --settings /data/solidsense/navigation/conf/settings.yml -d /data/solidsense/navigation
Restart=always
RestartSec=6

[Install]
WantedBy=multi-user.target